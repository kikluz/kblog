title: Middleman Blog
---
<!--  -->
  

    <% blog.articles[0...5].each do |article| %>
    <h1>
      <%= link_to article.title, article.path, :relative => true %>
      <time><%= article.date.strftime('%b %e %Y') %></time>
    </h1>
    <%= article.summary %>
    <%= link_to "Read more", article.url, :relative => true %>
  
    <% end %>
  




